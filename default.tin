#session rod mud.rod.org 3000
#split 10 1 0 -200 1

#ticker {tic} {save} {300}

#action {The frost cloak covering you melts away.} {cast frost cloak me}
#action {Your Water Shield Spell Expires!} {cast water shield me}
#action {You feel your resistance to %1 leave you.} {cast resist %1 me}
#action {The ice armour surrounding you melts away.} {cast ice armour me}




#alias ef {effects}
#alias re {reply %0}
#alias orgin {n;n;n;move curtain;u;u;u;n;ne;nw;enter statue;in;in;s}
#alias {buff} NOW TRIGGERS {get 7 str potions from bp;#7 drink str potion;get 7 int potions from bp;#7 drink int potion}
#alias {def} {
		cast ice armour %1;
		#delay {3} {cast frost cloak %1};
		#delay {6} {cast water shield %1};
		#delay {9} {cast resist magic %1};
		#delay {12} {cast resist cold %1};
	    }

#alias ac {cast ice armour me}
#alias ac2 {cast frost cloak me}
#alias shi {cast water shield me}
#alias rm {cast resist magic me}
#alias rc {cast resist cold me}

