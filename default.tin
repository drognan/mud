#ACTION {~%1 tells you%*: "%2}
{
     showtowin %1 tells you: "%2
}

#ACTION {~%1 says%*: "%2}
{
     showtowin %1 says: "%2
}

#ALIAS {showtowin}
{
     #list window del 1;
     #list window ins -1 {%0};
     #draw green rounded box 1 1 10 201 $window[-10..-1]
}

